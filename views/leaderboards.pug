extends layout

block content
    //h5(text-align="center") Coming Soon!
    .leaderboard
        h4 Leaderboards
        ul
            li.title Super Smash Bros. Ultimate
            li.rank
            li(data-rel='X6' data-value='6')
                span.name Xerox64
            li(data-rel='DK' data-value='12')
                span.name Davis Kong
            li(data-rel='SK' data-value='9')
                span.name Senekery
    script.
        $(document).ready(function() {
        var lb = $(".leaderboard").jqleaderboard({
        // options
        });
        lb_object = lb.data("leaderboard");
        lb_object.leaderboard_highlight("X6").leaderboard_select("X6");
        });